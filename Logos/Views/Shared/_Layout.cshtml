<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - Logos</title>
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
	<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/Logos.styles.css" asp-append-version="true" />
	<link rel="stylesheet" href="~/css/bootstrap2.css" />
	<link rel="stylesheet" href="~/css/Index.css" />
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.2/font/bootstrap-icons.min.css">
</head>
<body>

	<main role="main" class="pb-3">
		@RenderBody()
	</main>

	<footer class="border-top footer text-muted mt-5">
		<div class="container">
			<div class="row">
				<div class="col-1">
					<ul class="nav flex-column">
						<li class="nav-item mb-2">
							<a asp-action="Index" asp-controller="Home" class="nav-link mt-1 text-body" style="font-size:24px; color:floralwhite" >Logos</a>
						</li>
					</ul>
				</div>
				<div class="col">
					<ul class="nav flex-column">
						<li class="nav-item mb-2">
							<a asp-action="Index" asp-controller="Home" class="nav-link text-muted">Home</a>
						</li>
						<li class="nav-item mb-2">
							<a asp-action="Dishes" asp-controller="Home" class="nav-link mt-1 text-muted">Catalog</a>
						</li>
						<li class="nav-item mb-2">
							<a asp-action="Login" asp-controller="Account" class="nav-link mt-1 text-muted">Sing in</a>
						</li>
					</ul>
				</div>
				<div class="col-3">
				</div>
				<div class="col">
					<div class="social d-flex justify-content-center mt-5">
						<a class="mx-5" href="#!"><i class="bi bi-twitter text-body" style=" font-size: 2em;"></i></a>
						<a class="mx-5" href="#!"><i class="bi bi-facebook text-body" style=" font-size: 2em;"></i></a>
						<a class="mx-5" href="#!"><i class="bi bi-instagram text-body" style=" font-size: 2em;"></i></a>
					</div>
				</div>
			</div>
		</div>
	</footer>

	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@await RenderSectionAsync("Scripts", required: false)
</body>
</html>
